# 各種ファイルのシンボリックリンクを貼る

ln -sf ./nvim ~/.config/nvim

ln -sf ~/dotfile/shell/.zshrc ~/
ln -sf ~/dotfile/shell/.zprofile ~/
ln -sf ~/dotfile/shell/paths ~/
ln -sf ~/dotfile/shell/aliases ~/

ln -sf ~/dotfile/git/.gitignore_global ~/
ln -sf ~/dotfile/git/.gitconfig ~/
ln -sf ~/dotfile/git/.commit_template ~/
